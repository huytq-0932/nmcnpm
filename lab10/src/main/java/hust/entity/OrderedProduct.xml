<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="main.java.hust.entity.OrderedProduct">
        <table name="ordered_product" schema="emarket" catalog=""/>
        <id-class class="main.java.hust.entity.OrderedProductPK"/>
        <attributes>
            <id name="orderId">
                <column name="order_id" precision="10"/>
            </id>
            <id name="productId">
                <column name="product_id" precision="10"/>
            </id>
            <basic name="quantity">
                <column name="quantity" precision="10"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>

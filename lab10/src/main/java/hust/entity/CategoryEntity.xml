<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="main.java.hust.entity.CategoryEntity">
        <table name="category" schema="emarket" catalog=""/>
        <attributes>
            <id name="categoryId">
                <column name="category_id" precision="10"/>
            </id>
            <basic name="name">
                <column name="name" length="45"/>
            </basic>
            <basic name="image">
                <column name="image" nullable="false" length="45"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
